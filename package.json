{
  "name": "cbase-utils",
  "version": "1.2.4",
  "description": "Base utils for ExpressJS framework",
  "author": "github@cforclown - Hafis Alrizal",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/cforclown/cbase-utils#readme"
  },
  "homepage": "https://github.com/cforclown/cbase-utils#readme",
  "bugs": {
    "url": "https://github.com/cforclown/cbase-utils/issues"
  },
  "files": [
    "lib/**/*"
  ],
  "keywords": [
    "Base",
    "Utils"
  ],
  "scripts": {
    "test": "jest",
    "test:coverage": "jest --coverage",
    "build": "tsc",
    "lint": "eslint \"src/**/*.{js,ts}\"",
    "lint:check": "npm run lint",
    "lint:format": "npm run lint -- --fix",
    "lint:staged": "lint-staged",
    "prepare": "npm run build",
    "prepublishOnly ": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "git add -A src",
    "postversion": "git push && git push --tags"
  },
  "dependencies": {
    "axios": "^1.3.4",
    "crypto-js": "^4.0.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "joi": "^17.5.0",
    "jsonwebtoken": "^9.0.0",
    "nanoid": "^3.1.30"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/crypto-js": "^4.0.2",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.3",
    "@types/jsonwebtoken": "^8.5.6",
    "@types/nanoid": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^5.15.0",
    "@typescript-eslint/parser": "^5.15.0",
    "eslint": "^7.32.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^26.1.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.2.0",
    "jest": "^27.4.4",
    "jest-mock-req-res": "^1.0.2",
    "lint-staged": "^13.2.0",
    "ts-jest": "^27.1.1",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.3"
  },
  "lint-staged": {
    "src/**/*.{js,ts}": [
      "npm run lint-check"
    ]
  },
  "volta": {
    "node": "18.15.0",
    "npm": "9.5.0"
  }
}
